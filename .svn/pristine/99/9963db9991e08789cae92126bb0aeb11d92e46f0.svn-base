.top {width:100%; height:146px; background:url(../images/tb1.jpg) repeat-x top;}
.top1 {width:1200px; height:36px; margin:0 auto;}
.top1_txt {float:left; color:#fff; margin-top:8px;}
.top1_tel {float:right; width:auto; padding-left:143px; background:url(../images/tb2.jpg) no-repeat; height:27px; color:#ffa900; font-size:14px; font-weight:bold; padding-top:9px;}

.tmenubox {width:1200px; margin:0 auto; height:110px;}
.logo {float:left; margin-top:10px;}
.tmenu {float:right; margin-top:35px;}
.tmenu ul li {float:left; width:96px; height:40px; line-height:40px; font-size:14px; font-weight:bold; margin-left:5px; text-align:center; position:relative; z-index:10000}
.tmenu ul li a.one { display:block; width:100%; height:40px;}
.tmenu ul li.hover a.one { background:#ffaa06; color:#fff}
.tmenu ul li p { position:absolute; left:0; top:40px; width:100%; height:auto;  background:#ffaa06; padding:2px 0px; display:none;}
.tmenu ul li p.hover { display:block;}
.tmenu ul li p a {display:block; width:100%; height:32px; text-align:center; line-height:32px; font-size:12px; color:#fff; overflow:hidden;}
.tmenu ul li p a:hover {background:#3b0101; text-decoration:underline}

.islide {width:100%; height:450px; position:relative;}
.islideimg {width:100%; height:450px; position:relative;}
.islideimg p {position:absolute; left:0; top:0; width:100%; height:450px; display:none;}
.islideimg p.hover { display:block;}
.islideimg p a { display:block; width:100%; height:450px;}
.islidebtn {width:100%; height:20px; padding-top:0px; position:absolute; bottom:20px; left:0;}
.islidebtn p {width:auto;  height:3px; margin:0 auto; position:absolute; left:auto; top:10px;}
.islidebtn p a {float:left; width:10px; height:10px; background:#aeaeae; margin-left:15px;border-radius:15px;-webkit-border-radius:15px;-moz-border-radius:15px;}
.islidebtn p a:hover { background:#ffa900;}
.islidebtn p a.hover { background:#ffa900;}

.icar_t {width:360px; height:80px; overflow:hidden; margin:0 auto; background:url(../images/tb4.jpg) no-repeat; position:relative;}
.icar_t p {font-size:14px; color:#939393; text-align:center; line-height:30px; width:100%; text-align:center; position:absolute; top:8px;}
.icar_t span {font-size:24px; color:#333;}
.icar_sort {width:100%; height:34px; position:relative; overflow:hidden}
.icar_sort p { position:absolute; left:0; top:0;}
.icar_sort p a {display:block; width:auto; float:left; height:34px; line-height:34px; padding:0px 20px; font-size:14px; color:#fff; background:#ffaa06; margin-left:5px; margin-right:5px;}
.icar_sort p a:hover { background:#888;}
.icar_sort p a.hover { background:#888;}
.icarlist {width:1200px; height:auto; margin:0 auto;}
.icarlist ul li {float:left; width:226px; height:290px; border:1px solid #e9e9e9; margin-left:15px; margin-top:15px;}
.icarlist ul li.nomar { margin-left:0;}
.icarlist ul li a.img {display:block; width:218px; height:160px; margin:4px; overflow:hidden;}
.icarlist ul li a.img img:hover {
	transform: scale(1.15);
    transition: all 1s ease 0s;
	-webkit-transform: scale(1.15);
	-webkit-transform: all 1s ease 0s;
}
.icarlist ul li p { width:92%; margin:0 auto; height:32px;}
.icarlist ul li .p1 {width:92%; height:32px; margin:0 auto; padding-top:5px;}
.icarlist ul li .p1 .t {float:left; font-size:16px;}
.icarlist ul li .p1 .pr {float:right; font-size:14px; color:#ffa900;}
.icarlist ul li .p3 a {display:block; width:47%; padding:8px 0px; background:#999; color:#fff; text-align:center;}
.icarlist ul li .p3 .da { float:left; }
.icarlist ul li .p3 .ya { float:right;}
.icarlist ul li .p3 a:hover { background:#ffaa06}
.icarmore {display:block; width:100px; padding:10px; margin:0 auto; font-size:18px; border:1px solid #c1c1c1; text-align:center}

