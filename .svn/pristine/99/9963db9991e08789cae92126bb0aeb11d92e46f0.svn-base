@charset "utf-8";
/* CSS Document */

body{ margin:0; padding:0; font-family:"微软雅黑",Arial; font-size:12px;  background-color:#fff; color:#2e2e2e;  margin:0 auto;}
ul{ margin:0; padding:0; list-style:none;}
li { margin:0; padding:0; list-style:none}
a{ color:#2e2e2e; text-decoration:none;}
