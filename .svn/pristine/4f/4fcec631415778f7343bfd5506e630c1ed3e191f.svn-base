<include file="Public/_header" title="代理等級列表"/>
<section class="Hui-article-box">
    <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> <a href="__MODULE__/index" class="maincolor">首頁</a>
        <span class="c-gray en">&gt;</span>

        <span class="c-gray en">&gt;</span>
        代理等級列表 <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a> </nav>
    <div class="Hui-article">
        <article class="cl pd-20">
            <form method="post" action="__CONTROLLER__/vip_index">
            <div class="text-c">
                <input type="text" class="input-text" style="width:250px" placeholder="輸入代理等級名字關鍵字" id="" name="key">
                <button type="submit" class="btn btn-success" id="" name=""><i class="Hui-iconfont">&#xe665;</i> 搜索</button>
            </div>
            </form>
            <div class="cl pd-5 bg-1 bk-gray mt-20">
                <span class="l"><!-- <a href="javascript:;" onclick="del('vip_del')"  class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量刪除</a>--> <a href="javascript:;" onclick="javascript:location.href='__CONTROLLER__/vip_add'" class="btn btn-primary radius"><i class="Hui-iconfont">&#xe600;</i> 添加代理等級</a> </span>
                <span class="r">共有数据：<strong>{$count}</strong> 条</span>
            </div>
            <table class="table table-border table-bordered table-bg">
                <thead>
                <tr>
                    <th scope="col" colspan="12">代理等級列表</th>
                </tr>
                <tr class="text-c">
                    <th width="25"><input class="checkAll" type="checkbox" name="" value="0"></th>
                    <th>ID</th>
                    <th >代理等級名字</th>
                    <th>代理等級權限(數字越小權限越大)</th>
                    <th>代理佣金分成</th>
                    <th >操作</th>
                </tr>
                </thead>
                <tbody>
                <foreach name="list" item="vo" >
                <tr class="text-c">
                    <td><input  type="checkbox" class="box" value="{$vo.user_id}" name="box" /></td>
                    <td>{$vo.vip_id}</td>
                    <td>{$vo.name}</td>
                    <td>{$vo.level}</td>
                    <td>{$vo.commission}</td>
                <td class="td-manage"><a title="编辑" href="javascript:location.href='__CONTROLLER__/vip_update/id/{$vo.vip_id}';" onclick="" class="ml-5" style="text-decoration:none"><i class="Hui-iconfont">&#xe6df;</i></a> <a title="删除" href="javascript:;" data="{$vo.vip_id}"  onclick="del2('{$vo.vip_id}','vip_del')" class="ml-5 " style="text-decoration:none"><i class="Hui-iconfont">&#xe6e2;</i></a></td>
                </tr>
                </foreach>
                </tbody>
            </table>
        </article>
        {$page}
    </div>
</section>

<include file="Public/_footer"  />