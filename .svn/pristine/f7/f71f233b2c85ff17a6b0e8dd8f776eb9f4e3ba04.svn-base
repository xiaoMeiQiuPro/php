<include file="Public/_header" title="用戶操作記錄"/>
<section class="Hui-article-box">
    <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> <a href="__MODULE__/index" class="maincolor">首頁</a>
        <span class="c-gray en">&gt;</span>

        <span class="c-gray en">&gt;</span>
        用戶操作記錄 <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a> </nav>
    <div class="Hui-article">
        <article class="cl pd-20">
            <form method="post" action="__CONTROLLER__/log_index">
                <div class="text-c">時間搜索: &nbsp;
                    <input name="start" style="width: 160px;" type="text" id="datemin" onClick="jeDate({dateCell:'#datemin',isTime:true,format:'YYYY-MM-DD hh:mm:ss'})"  class="input-text Wdate" style="width:120px;">
                    -
                    <input style="width: 160px;" name="end" type="text" onClick="jeDate({dateCell:'#datemax',isTime:true,format:'YYYY-MM-DD hh:mm:ss'})" id="datemax" class="input-text Wdate" style="width:120px;">
                    <button type="submit" class="btn btn-success" id="" name="search"><i class="Hui-iconfont">&#xe665;</i> 搜索</button>
                </div>
            </form>
            <div class="cl pd-5 bg-1 bk-gray mt-20">
                <!--  <span class="l"> <a href="javascript:;" onclick="del()"  class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量刪除</a> <a href="javascript:;" onclick="javascript:location.href='__CONTROLLER__/add'" class="btn btn-primary radius"><i class="Hui-iconfont">&#xe600;</i> 添加管理員</a> </span>
                -->  <span class="r">共有数据：<strong>{$count}</strong> 条</span>
            </div>
            <table class="table table-border table-bordered table-bg">
                <thead>
                <tr>
                    <th scope="col" colspan="9">用戶操作記錄</th>
                </tr>
                <tr class="text-c">
                    <!--<th width="25"><input class="checkAll" type="checkbox" name="" value="0"></th>-->
                    <th width="40">ID</th>
                    <th width="150">操作記錄</th>
                    <th width="110">操作時間</th>
                    <th width="150">操作IP</th>
                </tr>
                </thead>
                <tbody>
                <foreach name="list" item="vo" >
                    <tr class="text-c">
                        <!--<td><input <eq name="vo.admin_id" value="1" >disabled</eq> type="checkbox" class="box" value="{$vo.admin_id}" name="box"></td>-->
                        <td>{$vo.user_log_id}</td>
                        <td>{$vo.record}</td>
                        <td>{$vo.date}</td>
                        <td>{$vo.ip}</td>
                    </tr>
                </foreach>
                </tbody>
            </table>
        </article>
        {$page}
    </div>
</section>

<include file="Public/_footer"  />